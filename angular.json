{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "serWebManagement": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "smc",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "./bin/extra-webpack.config.js"
            },
            "baseHref": "/extensions/ser-web-management/",
            "outputPath": "dist/serWebManagement",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/assets/favicon.ico",
              "src/assets"
            ],
            "styles": [{
                "input": "node_modules/@angular/material/prebuilt-themes/indigo-pink.css"
              },
              "src/skin/scss/main.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [{
                "replace": "src/environments/environment.ts",
                "with": "src/environments/environment.prod.ts"
              }],
              "optimization": true,
              "outputHashing": "none",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            },
            "en": {
              "aot": true,
              "outputPath": "dist/smc/en",
              "i18nFile": "src/i18n/en_i18n.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "en"
            },
            "de": {
              "aot": true,
              "outputPath": "dist/smc/de",
              "i18nFile": "src/i18n/de_i18n.xlf",
              "i18nFormat": "xlf",
              "i18nLocale": "de"
            }
          }
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "browserTarget": "serWebManagement:build",
            "baseHref": "/extensions/ser-web-management/"
          },
          "configurations": {
            "en": {
              "browserTarget": "serWebManagement:build:en"
            },
            "de": {
              "browserTarget": "serWebManagement:build:de"
            },
            "production": {
              "browserTarget": "serWebManagement:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "serWebManagement:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [{
                "input": "node_modules/@angular/material/prebuilt-themes/indigo-pink.css"
              },
              "src/skin/scss/main.scss"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "serWebManagement-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "serWebManagement:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "serWebManagement:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "serWebManagement",
  "cli": {
    "warnings": {
      "typescriptMismatch": false
    }
  }
}
